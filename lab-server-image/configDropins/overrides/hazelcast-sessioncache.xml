<server>
    <featureManager>
        <feature>sessionCache-1.0</feature>
    </featureManager>
    <library id="HazelcastLib">
        <!-- <file name="${shared.resource.dir}/hazelcast/hazelcast.jar" /> -->
        <fileset dir="${shared.resource.dir}/hazelcast"/>
